{
  "name": "git-rename",
  "publisher": "ambooth",
  "displayName": "git-rename",
  "description": "Rename files using git-mv",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.git-rename"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder == false",
          "command": "extension.git-rename",
          "group": "7_modification"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.git-rename",
        "title": "Rename (git-mv) [TS]"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "spawn-command": "0.0.2-1",
    "tree-kill": "^1.2.2"
  }
}
